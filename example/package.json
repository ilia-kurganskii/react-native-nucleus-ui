{
  "name": "nucleus-ui-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "detox:build:ios": "detox build -c ios",
    "detox:build:ios-ci": "detox build -c ios.ci",
    "detox:build:ios-tdd": "detox build -c ios.tdd",
    "detox:build:android": "detox build -c android",
    "detox:build:android-tdd": "detox build -c android.tdd",
    "detox:test:ios": "detox test -c ios",
    "detox:test:ios-ci": "detox test -c ios.ci",
    "detox:test:android": "detox test -c android",
    "detox:test:ios-tdd": "detox test -c ios.tdd",
    "detox:test:android-tdd": "detox test -c android.tdd",
    "detox:cache": "detox build-framework-cache",
    "detox:build": "yarn detox:build:ios && yarn detox:build:android",
    "detox:test": "yarn detox:test:ios && yarn detox:test:android"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/runtime": "7.20.6",
    "@react-native-community/eslint-config": "3.2.0",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.14",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.26",
    "@storybook/channel-websocket": "6.5.14",
    "@storybook/react-native": "5.3.27",
    "@storybook/react-native-server": "5.3.23",
    "@types/jest": "29.2.4",
    "@types/jest-image-snapshot": "6.1.0",
    "@types/pixelmatch": "5.2.4",
    "@types/react-native": "0.67.12",
    "@types/react-test-renderer": "17.0.2",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "babel-jest": "29.3.1",
    "babel-loader": "9.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "19.13.0",
    "eslint": "8.30.0",
    "jasmine": "4.5.0",
    "jest": "29.3.1",
    "jest-circus": "29.3.1",
    "jest-image-snapshot": "6.1.0",
    "metro-react-native-babel-preset": "0.73.6",
    "pixelmatch": "5.3.0",
    "pngjs": "6.0.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "17.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4",
    "ws": "8.11.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
