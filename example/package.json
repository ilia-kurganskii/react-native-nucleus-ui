{
  "name": "nucleus-ui-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "detox:build:ios": "detox build -c ios",
    "detox:build:ios-ci": "detox build -c ios.ci",
    "detox:build:ios-tdd": "detox build -c ios.tdd",
    "detox:build:android": "detox build -c android",
    "detox:build:android-tdd": "detox build -c android.tdd",
    "detox:test:ios": "detox test -c ios",
    "detox:test:ios-ci": "detox test -c ios.ci",
    "detox:test:android": "detox test -c android",
    "detox:test:ios-tdd": "detox test -c ios.tdd",
    "detox:test:android-tdd": "detox test -c android.tdd",
    "detox:cache": "detox build-framework-cache",
    "detox:build": "yarn detox:build:ios && yarn detox:build:android",
    "detox:test": "yarn detox:test:ios && yarn detox:test:android"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/runtime": "7.18.6",
    "@react-native-community/eslint-config": "3.0.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.25",
    "@storybook/channel-websocket": "6.5.9",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@types/jest": "28.1.4",
    "@types/jest-image-snapshot": "5.1.0",
    "@types/pixelmatch": "5.2.4",
    "@types/react-native": "^0.67.8",
    "@types/react-test-renderer": "17.0.2",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "babel-jest": "28.1.2",
    "babel-loader": "8.2.5",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "19.7.1",
    "eslint": "^8.19.0",
    "jasmine": "4.2.1",
    "jest": "28.1.2",
    "jest-circus": "28.1.2",
    "jest-image-snapshot": "5.1.0",
    "metro-react-native-babel-preset": "0.71.2",
    "pixelmatch": "5.3.0",
    "pngjs": "6.0.0",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4",
    "ws": "8.8.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
